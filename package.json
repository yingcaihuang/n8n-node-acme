{
  "name": "n8n-nodes-acme",
	"version": "1.4.0",
  "description": "n8n node for ACME certificate issuance with DNS validation support",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "acme",
    "ssl",
    "tls",
    "certificate",
    "dnspod",
    "aliyun",
    "dns"
  ],
  "license": "MIT",
  "homepage": "https://github.com/betty/n8n-node-acme#readme",
  "author": {
    "name": "betty",
    "email": "betty@example.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/betty/n8n-node-acme.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && npm run copy-icons",
    "copy-icons": "copyfiles -u 1 icons/* dist/",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build && npm run lint -s"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
		"credentials": [
			"dist/credentials/DnspodApi.credentials.js",
			"dist/credentials/AliyunApi.credentials.js",
			"dist/credentials/CloudflareApi.credentials.js",
			"dist/credentials/Route53Api.credentials.js",
			"dist/credentials/BaiduApi.credentials.js",
			"dist/credentials/HuaweiApi.credentials.js"
		],
    "nodes": [
      "dist/nodes/AcmeCertificate/AcmeCertificate.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^18.16.17",
    "@typescript-eslint/parser": "^5.59.9",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "copyfiles": "^2.4.1",
    "eslint": "^8.42.0",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "prettier": "^2.8.8",
    "typescript": "^5.1.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "acme-client": "^4.2.5",
    "axios": "^1.4.0",
    "node-forge": "^1.3.1"
  }
}
